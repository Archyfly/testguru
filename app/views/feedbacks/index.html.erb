<h1 align="center"><b>Show all feedbacks</b></h1>

<table class="table table-striped table-light">
  <tr>
    <th><b>User email:</b></th>
    <th><b>feedback<b></th>
  </tr>

  <% @feedbacks.each do |fb| %>
    <tr>
      <td><%= fb.user.email %></td>
      <td><%= fb.feedback %></td>
    </tr>
  <% end %>
</table>

<br/>
