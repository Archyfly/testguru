<h1>Show all questions</h1>

<table border="1">
  <tr>
    <th>Question title</th>
    <th>Question created at</th>

    <th>Delete question</th>
  </tr>

  <% @questions.each do |question| %>
    <tr>
      <td><%= question.title %></td>
      <td><%= question.created_at %></td>

    </tr>
  <% end %>

</table>
<br/>
