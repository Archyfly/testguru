<tr>
  
  <td><%= test.title %></td>
  <td><%= test.level %></td>

  <td><%= link_to t('.show'), admin_test_path(test) %></td>
  <td><%= link_to "Edit", edit_admin_test_path(test) %></td>

  <td>
    <%= link_to 'Delete',
    admin_test_path(test),
    method: :delete,
    data: { confirm: 'This test will be deleted! Are you sure?' } %>
  </td>
  <td> <%= test.questions.count %></td>
</tr>
