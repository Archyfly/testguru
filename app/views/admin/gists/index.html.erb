<h1>Show all Gists</h1>

<table class="table table-striped table-dark" border = "1">
  <tr>
    <th>user</th>
    <th>question</th>
    <th>gisturl</th>

  </tr>

  <% @gists.each do |gist| %>
    <tr>
      <td><%= gist.user.email %></td>
      <td><%= gist.question.title %></td>
      <td><%= gist.gisturl %></td>

    </tr>
  <% end %>
</table>

<br/>
