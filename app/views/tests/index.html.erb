<h1>Tricky tests</h1>

<table border = "1">
  <tr>
    <th>Title</th>
    <th>Level</th>
  </tr>

  <% @tests.each do |test| %>
    <tr>
      <td><%= test.title %></td>
      <td><%= test.level %></td>
    </tr>
  <% end %>
</table>



