<tr>
  <td><%= test.id %></td>
  <td><%= test.title %></td>
  <td><%= test.level %></td>

  <td><%= link_to "Show", test_path(test) %></td>
  <td><%= link_to "Edit", edit_test_path(test) %></td>
  <td><%= link_to 'Delete',
    test_path(test),
    method: :delete,
    data: { confirm: 'This test will be deleted! Are you sure?' } %>
  </td>
  <td> <%= test.questions.count %></td>
</tr>
